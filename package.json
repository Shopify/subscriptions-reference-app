{
  "name": "subscriptions-app-remix",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production vite build && NODE_ENV=production vite build --ssr",
    "predev": "dotenv -c development prisma generate && dotenv -c development prisma migrate deploy",
    "dev": "dotenv -c development shopify app dev",
    "local:dev": "SHOPIFY_CLI_1P_DEV=1 shopify app dev",
    "generate": "shopify app generate",
    "env": "shopify app env",
    "start": "remix-serve ./build/server/index.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "prisma": "prisma",
    "setup": "prisma generate && prisma migrate deploy",
    "db:migrate": "prisma migrate deploy",
    "test": "LC_ALL=en_US.UTF-8 dotenv -c test vitest",
    "test:ci:buyer-subscriptions": "LC_ALL=en_US.UTF-8 dotenv -c ci vitest extensions/buyer-subscriptions",
    "test:ci:admin-subs-action": "LC_ALL=en_US.UTF-8 dotenv -c ci vitest extensions/admin-subs-action",
    "type-check": "tsc --project tsconfig.json && tsc --project extensions/buyer-subscriptions/tsconfig.json && tsc --project extensions/admin-subs-action/tsconfig.json",
    "graphql-codegen": "graphql-codegen",
    "graphql-codegen-customer": "pnpm graphql-codegen --project shopifyCustomerApi",
    "graphql-codegen-admin-ext": "pnpm graphql-codegen --project adminSubsActionExtension"
  },
